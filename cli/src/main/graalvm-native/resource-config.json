{
  "resources": {
    "includes": [
      { "pattern": "version.properties" },
      { "pattern": "io/github/bmarwell/jfmt/config/default-config.xml" },
      { "pattern": "io/github/bmarwell/jfmt/config/equalsverifier.xml" },
      { "pattern": "org/eclipse/jdt/internal/compiler/messages(.*)?\\.properties" },
      { "pattern": "org/eclipse/jdt/internal/compiler/problem/messages(.*)?\\.properties" },
      { "pattern": "org/eclipse/jdt/internal/compiler/impl/messages(.*)?\\.properties" },
      { "pattern": "org/eclipse/jdt/internal/compiler/problem/.*\\.properties" },
      { "pattern": "org/eclipse/jdt/internal/compiler/parser/.*\\.rsc" },
      { "pattern": "org/eclipse/jdt/internal/compiler/parser/readableNames\\.props" },
      { "pattern": "org/eclipse/jdt/internal/compiler/parser/unicode.*/.*\\.rsc" },
      { "pattern": "org/eclipse/jdt/internal/compiler/util/messages(.*)?\\.properties" },
      { "pattern": "org/eclipse/jdt/internal/formatter/.*\\.properties" }
    ]
  },
  "bundles": [
    { "name": "org.eclipse.jdt.internal.compiler" },
    { "name": "org.eclipse.jdt.internal.compiler.messages" },
    { "name": "org.eclipse.jdt.internal.compiler.batch.messages" },
    { "name": "org.eclipse.jdt.internal.compiler.impl.messages" },
    { "name": "org.eclipse.jdt.internal.compiler.parser.messages" },
    { "name": "org.eclipse.jdt.internal.compiler.problem.messages" },
    { "name": "org.eclipse.jdt.internal.compiler.util.messages" },
    { "name": "org.eclipse.jdt.internal.formatter.messages" }
  ]
}
